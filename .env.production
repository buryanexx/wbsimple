# Настройки для продакшен-среды
NODE_ENV=production

# API URL для продакшена
API_URL=https://api.your-production-server.com

# Режим маршрутизации (hash для Telegram WebApp)
VITE_USE_HASH_ROUTER=true

# Настройки для Telegram WebApp
VITE_TELEGRAM_BOT_USERNAME=wbsimple_bot

# Отключение логирования в консоль
VITE_DISABLE_LOGS=true

# Метаданные приложения
VITE_APP_TITLE="WB Simple"
VITE_APP_DESCRIPTION="Образовательная платформа для обучения заработку на Wildberries" 